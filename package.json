{
  "name": "brand-toolkit-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "analyze": "yarn build && mkdir -p explorer && source-map-explorer 'build/**/*.js' > explorer/index.html",
    "clean": "rm -rf ./node_modules && yarn install",
    "coverage": "yarn test --coverage && jest-coverage-badges",
    "test": "jest --env=jsdom --verbose --maxWorkers=2 --logHeapUsage",
    "test:watch": "yarn test --watch",
    "start": "nodemon --trace-warnings ",
    "start:production": "NODE_ENV=production node build/index.js --trace-warnings",
    "build": "rm -rf build && tsc -p tsconfig.json",
    "build:dev": "NODE_ENV=develoment tsc -p tsconfig.json",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "pretty-quick": "pretty-quick"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint --fix && yarn pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
